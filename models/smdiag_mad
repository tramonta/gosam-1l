[Bbar, B, g;   gs='+1', gw='0', QCD='+1', QED='0', RK='0']
[Tbar, T, g;   gs='+1', gw='0', QCD='+1', QED='0', RK='0']

[g, g, g, g;   gs='+2', gw='0', QCD='+2', QED='0', RK='0']
[g, g, g;      gs='+1', gw='0', QCD='+1', QED='0', RK='1']
[ghbar, gh, g; gs='+1', gw='0', QCD='+1', QED='0', RK='1']

